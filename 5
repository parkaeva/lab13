#include <iostream>
using namespace std;
int main()
{
    int n, i, t, b;
    cin>>n;
    int a[i];
    for (i=0; i<n; i++)
    {
        cin>>a[i];
    }
    for (i=0; i<n; i++)
    {
        if(i%2!=0)
            {
                cout<<a[i]<<" ";
            }
    }
    for (i=n; i>=0; i--)
    {
        if(i%2==0)
            {
                cout<<a[i]<<" ";
            }
    }
    return 0;
}
