#include <iostream>
using namespace std;
int main()
{
    int A, B, k, n;
    cin>>n>>A>>B;
    int *a=new int[n];
    k=0;
    a[0]=A;
    a[1]=B;
    a[2]=a[0]+a[1];
    for(int i=3; i<n; i++)
    {
        a[i]=a[i-1]*2;
    }
    for(int i=0; i<n; i++)
    {
        cout<<a[i]<<" ";
    }

    return 0;
}
